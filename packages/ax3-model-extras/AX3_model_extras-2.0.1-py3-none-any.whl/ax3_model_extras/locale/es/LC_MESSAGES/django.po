#: ax3_model_extras/fields.py:50
msgid "The allowed quality is from 0 to 100"
msgstr "La calidad permitida es de 0 a 100"

#: ax3_model_extras/fields.py:60
msgid "Image too small to be scaled"
msgstr "Imagen demasiado pequeña para ser escalada"

#: ax3_model_extras/fields.py:62
msgid "The image is invalid or corrupted"
msgstr "La imagen no es válida o está dañada"

#: ax3_model_extras/fields.py:71
msgid "Image format unsupported"
msgstr "Formato de imagen no soportado"

#: ax3_model_extras/fields.py:75
msgid "GIF image size unsupported"
msgstr "Tamaño de imagen GIF no soportado"

#: ax3_model_extras/validators.py:12
msgid "The weight of the file must be less than the maximum allowed size"
msgstr "El peso del archivo debe ser menor al tamaño máximo permitido"

#: ax3_model_extras/validators.py:22
msgid "The image must have the specified dimensions"
msgstr "La imagen debe tener las dimensiones especificadas"

#: ax3_model_extras/validators.py:39
msgid "The image must have at least the specified dimensions"
msgstr "La imagen debe tener al menos las dimensiones especificadas"

#: ax3_model_extras/validators.py:57
msgid "The document does not have the correct format"
msgstr "El documento no tiene el formato correcto"

#: ax3_model_extras/validators.py:70
msgid "Invalid phone number."
msgstr "Teléfono invalido."
