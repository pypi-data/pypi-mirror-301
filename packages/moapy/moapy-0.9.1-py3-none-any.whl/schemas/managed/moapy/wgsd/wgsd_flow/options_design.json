{
  "openapi": "3.1.0",
  "info": {
    "title": "moapy",
    "description": "Schema for moapy",
    "version": "0.9.0.6"
  },
  "servers": [
    {
      "url": "https://moa.rpm.kr-dv-midasit.com/backend/python-executor/"
    },
    {
      "url": "https://moa.rpm.kr-st-midasit.com/backend/function-executor/python-execute/"
    }
  ],
  "paths": {
    "/execute?functionId=moapy/wgsd/wgsd_flow/options_design": {
      "post": {
        "summary": "Options Design",
        "description": "Return the options\n\nArgs:\n    opt (PMOptions): options\n\nReturns:\n    PMOptions: options",
        "operationId": "options design",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/options_design"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PMOptions"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "PMOptions": {
        "properties": {
          "dgncode": {
            "type": "string",
            "enum": [
              "ACI318M-19",
              "Eurocode2-04"
            ],
            "title": "Dgncode",
            "description": "Design code",
            "default": "Eurocode2-04"
          },
          "by_ecc_pu": {
            "type": "string",
            "enum": [
              "ecc",
              "P-U"
            ],
            "title": "By Ecc Pu",
            "description": "ecc or P-U",
            "default": "ecc"
          }
        },
        "type": "object",
        "title": "GSD Options",
        "dataclassname": "moapy.data_pre.PMOptions"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      },
      "options_design": {
        "properties": {
          "opt": {
            "$ref": "#/components/schemas/PMOptions"
          }
        },
        "type": "object",
        "required": [
          "opt"
        ],
        "title": "options design"
      }
    }
  }
}