[logging]
level = "debug"

[session]
ttl = "90d"
cookie = "_{{ project_name }}_session_id"

[database]
uri = "mongodb://localhost:27017/{{ project_name }}"
timeout = 1.1 # 1.1 seconds

[cache]
level1 = "request_local"
level2 = "simple"

[jwt]
enabled = false
secret_key = "-"

[tasks]
redis_url = "redis://127.0.0.1:6379/0"
queue = "{{ project_name }}_tasks"

